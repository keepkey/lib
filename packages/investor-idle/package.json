{
  "name": "@keepkey/investor-idle",
  "version": "2.1.6",
  "description": "An api layer for interaction with the ShapeShift Idle Router Contract and the Idle Vault contracts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "lib": "dist",
    "src": "src"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14.18.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "yarn clean && yarn compile",
    "clean": "rm -rf dist && rm -rf tsconfig.build.tsbuildinfo",
    "compile": "tsc -p tsconfig.build.json",
    "dev": "tsc --watch",
    "prepare": "yarn build",
    "test": "jest --coverage",
    "cli": "yarn build && node ./dist/idlecli.js"
  },
  "dependencies": {
    "@ethersproject/providers": "^5.5.3",
    "bignumber.js": "^9.0.2",
    "lodash": "^4.17.21",
    "web3": "1.7.4",
    "web3-core": "1.7.4",
    "web3-utils": "1.7.4"
  },
  "peerDependencies": {
    "@keepkey/caip": "^8.4.2",
    "@keepkey/chain-adapters": "^10.0.0",
    "@keepkey/investor": "^3.0.0",
    "@keepkey/types": "^8.3.1",
    "@shapeshiftoss/hdwallet-core": "^1.36.0"
  },
  "devDependencies": {
    "@keepkey/caip": "^8.4.5",
    "@keepkey/chain-adapters": "^10.2.2",
    "@keepkey/investor": "^3.0.2",
    "@keepkey/types": "^8.3.3",
    "@keepkey/unchained-client": "^10.3.1",
    "@shapeshiftoss/hdwallet-core": "^1.36.0"
  }
}
